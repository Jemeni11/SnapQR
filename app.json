{
  "expo": {
    "name": "SnapQR",
    "slug": "snapqr",
    "description": "Beyond Scanning - Remake, Share, and Explore",
    "scheme": "snapqr",
    "githubUrl": "https://github.com/Jemeni11/SnapQR",
    "orientation": "portrait",
    "version": "1.0.0",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "primaryColor": "#ACA7CB",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "SnapQR needs access to your Camera."
        }
      ],
      "expo-router"
    ]
  }
}
